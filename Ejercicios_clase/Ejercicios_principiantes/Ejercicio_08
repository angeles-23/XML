<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE estudiantes[
        <!ELEMENT estudiantes (estudiante+)>
        <!ELEMENT estudiante (nombre, apellido, fechaNacimiento, listaCalificaciones)> 
        <!ELEMENT nombre (#PCDATA)> 
        <!ELEMENT apellido (#PCDATA)> 
        <!ELEMENT fechaNacimiento (#PCDATA)> 
        <!ELEMENT listaCalificaciones (#PCDATA)> 
        <!ELEMENT listaCalificaiones (valorNumerico, fecha)> 
        <!ATTLIST estudiante 
                estatus (activo|inactivo) #REQUIRED
        > 
        <!ATTLIST listaCalificaiones 
                numero CDATA #REQUIRED
                fecha CDATA #IMPLIED
        > 
]>


